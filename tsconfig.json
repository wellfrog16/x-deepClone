{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "lib": [
        "es6",
        "dom",
        "es2015.promise"
      ],
      "allowJs": false,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "declaration": true,
      "sourceMap": true,
      "outDir": "./dist/",
      "removeComments": true,
      "strict": true,
      "noImplicitAny": false,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "noEmit": false,
    },
  }